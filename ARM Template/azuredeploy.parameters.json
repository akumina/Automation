    "contentVersion": "1.0.0.0",
    "parameters": {
        "webName": {
            "value": "",
            "metadata": {
                "description": "Name of the Web App."
            }
        },
        "webSku": {
            "value": "",
            "metadata": {
                "description": "Pricing tier of the Web App."
            }
        },
        "packageUri": {
            "value": "",
            "metadata": {
                "description": "Web App Package Uri."
            }
